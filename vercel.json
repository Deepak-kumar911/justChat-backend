{ 
    "version":2,
    "builds":[
        {
            "src":"index.js","use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/public/(.*)",
            "dest":"/public/$1"
            },
            {
            "src":"/(.*)",
            "dest":"/index.js"
            }
        ],
    "functions":{
        "api/**/*.js":{
            "headers":{
                "Access-Control-Allow-Origin":"*",
                "Access-Control-Allow-Methods":"GET, POST, OPTION",
                "Access-Control-Allow-Headers":"Origin, X-Requested-With, Content-Type, Accept"

            }
        }
    }
}